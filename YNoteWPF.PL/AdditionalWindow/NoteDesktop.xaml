        mc:Ignorable="d"
        Name="MainWindow"
        Title="NoteDesktop" Height="Auto" MinHeight="50" Width="Auto" MinWidth="50" WindowStyle="None" Background="{x:Null}" SizeChanged="Window_SizeChanged">
