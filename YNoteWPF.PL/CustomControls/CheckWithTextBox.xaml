<UserControl x:Class="YNoteWPF.PL.CustomControls.CheckWithTextBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:YNoteWPF.PL.CustomControls"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="40">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <CheckBox Name="CheckBoxElem" HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <TextBox Name="TextBoxElem" HorizontalAlignment="Center" VerticalAlignment="Center" Width="Auto" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="Black"/>
        </StackPanel>
    </Grid>
</UserControl>
